{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "Logging": {
      "LogLevel": "Info"
    },
    "Polly": {
      "RetryDelaySeconds": 3,
      "MaxRetryAttempts": 4,
      "TimeoutSeconds": 30,
      "RateLimitPermitLimit": 30,
      "RateLimitQueue": 10,
      "RateLimitSegmentsPerWindow": 5,
      "RateLimitWindowSeconds": 60
    },
    "S3": {
      "FileBucketName": "incontrol-file-bucket",
      "Region": "us-east-2"
    },
    "DynamoDB": {
      "Region": "us-east-2"
    }
  }
}
